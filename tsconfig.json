{
  "compilerOptions": {
    "allowJs": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "isolatedModules": true,
    "target": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "ziggy-js": [
        "./vendor/tightenco/ziggy"
      ]
    },
    "types": [
      "cypress",
      "node",
      "vite/client"
    ] // Tambahkan "vite/client" di sini
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}